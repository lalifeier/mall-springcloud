{
  "configurations": [
    {
      "type": "java",
      "name": "MallAccountApplication",
      "request": "launch",
      "mainClass": "com.github.lalifeier.mall.account.interfaces.MallAccountApplication",
      "projectName": "account-service",
      "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=42943 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=account-service"
    },
    {
      "type": "java",
      "name": "MallMonitorApplication",
      "request": "launch",
      "mainClass": "com.github.lalifeier.mall.monitor.MallMonitorApplication",
      "projectName": "mall-monitor"
    },
    {
      "type": "java",
      "name": "MallAuthApplication",
      "request": "launch",
      "mainClass": "com.github.lalifeier.mall.auth.MallAuthApplication",
      "projectName": "auth-service"
    },
    {
      "type": "java",
      "name": "Spring Boot-MallGatewayApplication<mall-gateway>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "com.github.lalifeier.mall.gateway.MallGatewayApplication",
      "projectName": "mall-gateway",
      "args": "",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "java",
      "name": "Spring Boot-MallDemoApplication<mall-demo>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
